<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#0e7490" />
      <stop offset="100%" stop-color="#0f172a" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Dividend Investing</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#94a3b8">Building a Passive Income Stream with Dividend Stocks</text>
  
  <!-- Graphics for dividend investing -->
  <!-- Main stock bar with upward movement -->
  <rect x="150" y="200" width="60" height="150" fill="#06b6d4" rx="3" />
  
  <!-- Dividend payment bars -->
  <rect x="250" y="280" width="60" height="70" fill="#22c55e" rx="3" />
  <rect x="350" y="260" width="60" height="90" fill="#22c55e" rx="3" />
  <rect x="450" y="240" width="60" height="110" fill="#22c55e" rx="3" />
  <rect x="550" y="220" width="60" height="130" fill="#22c55e" rx="3" />
  
  <!-- Dividend arrows -->
  <path d="M180,200 L250,280" stroke="#ffffff" stroke-width="2" stroke-dasharray="5,3" />
  <path d="M180,200 L350,260" stroke="#ffffff" stroke-width="2" stroke-dasharray="5,3" />
  <path d="M180,200 L450,240" stroke="#ffffff" stroke-width="2" stroke-dasharray="5,3" />
  <path d="M180,200 L550,220" stroke="#ffffff" stroke-width="2" stroke-dasharray="5,3" />
  
  <!-- Annotation text -->
  <text x="152" y="370" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Stock</text>
  <text x="240" y="370" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Div Yr 1</text>
  <text x="340" y="370" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Div Yr 2</text>
  <text x="440" y="370" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Div Yr 3</text>
  <text x="540" y="370" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Div Yr 4</text>
  
  <!-- Total return annotation -->
  <rect x="600" y="150" width="150" height="90" rx="10" fill="#ffffff" fill-opacity="0.1" />
  <text x="620" y="180" font-family="Inter, sans-serif" font-size="16" fill="#ffffff">Total Return:</text>
  <text x="620" y="210" font-family="Inter, sans-serif" font-size="16" fill="#ffffff">Stock Growth +</text>
  <text x="620" y="235" font-family="Inter, sans-serif" font-size="16" fill="#ffffff">Dividend Income</text>
  
  <!-- Coin icons representing dividends -->
  <circle cx="280" cy="250" r="15" fill="#fbbf24" />
  <circle cx="280" cy="250" r="12" fill="#fcd34d" />
  <text x="275" y="254" font-family="Inter, sans-serif" font-size="12" fill="#0f172a">₹</text>
  
  <circle cx="380" cy="230" r="15" fill="#fbbf24" />
  <circle cx="380" cy="230" r="12" fill="#fcd34d" />
  <text x="375" y="234" font-family="Inter, sans-serif" font-size="12" fill="#0f172a">₹</text>
  
  <circle cx="480" cy="210" r="15" fill="#fbbf24" />
  <circle cx="480" cy="210" r="12" fill="#fcd34d" />
  <text x="475" y="214" font-family="Inter, sans-serif" font-size="12" fill="#0f172a">₹</text>
  
  <circle cx="580" cy="190" r="15" fill="#fbbf24" />
  <circle cx="580" cy="190" r="12" fill="#fcd34d" />
  <text x="575" y="194" font-family="Inter, sans-serif" font-size="12" fill="#0f172a">₹</text>
</svg> 