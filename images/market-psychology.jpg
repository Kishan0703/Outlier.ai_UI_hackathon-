<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#be123c" />
      <stop offset="100%" stop-color="#0f172a" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Market Psychology</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#fda4af">Mastering Emotions for Better Returns</text>
  
  <!-- Graphics for market psychology -->
  <!-- Psychology cycle curve -->
  <path d="M100,250 C150,200 200,150 250,170 C300,190 320,200 350,170 C380,140 400,100 450,130 C500,160 520,190 550,240 C580,290 600,350 650,280 C700,210 720,200 750,250" 
        fill="none" stroke="#ffffff" stroke-width="3" />
  
  <!-- Emotional state labels along the curve -->
  <text x="120" y="210" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Optimism</text>
  <text x="200" y="140" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Excitement</text>
  <text x="280" y="160" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Thrill</text>
  <text x="350" y="140" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Euphoria</text>
  <text x="420" y="110" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Anxiety</text>
  <text x="480" y="150" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Denial</text>
  <text x="530" y="220" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Panic</text>
  <text x="580" y="300" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Despondency</text>
  <text x="650" y="250" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Hope</text>
  <text x="720" y="230" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Relief</text>
  <text x="750" y="280" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">Optimism</text>
  
  <!-- Buy zone -->
  <circle cx="620" cy="290" r="20" fill="#22c55e" fill-opacity="0.5" />
  <text x="620" y="295" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">BUY</text>
  
  <!-- Sell zone -->
  <circle cx="420" cy="130" r="20" fill="#ef4444" fill-opacity="0.5" />
  <text x="420" y="135" font-family="Inter, sans-serif" font-size="14" fill="#ffffff" text-anchor="middle">SELL</text>
  
  <!-- Brain icon symbolizing psychology -->
  <circle cx="700" cy="100" r="40" fill="#ffffff" fill-opacity="0.1" />
  <circle cx="680" cy="85" r="10" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="700" cy="80" r="8" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="720" cy="85" r="12" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="675" cy="100" r="9" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="695" cy="105" r="11" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="715" cy="100" r="10" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="685" cy="120" r="12" fill="#fda4af" fill-opacity="0.5" />
  <circle cx="705" cy="125" r="9" fill="#fda4af" fill-opacity="0.5" />
  <path d="M670,85 C660,120 670,130 700,130 C730,130 740,120 730,85" fill="none" stroke="#fda4af" stroke-width="2" />
