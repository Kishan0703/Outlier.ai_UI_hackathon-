<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1e3a8a" />
      <stop offset="100%" stop-color="#0f172a" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Value Investing</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#94a3b8">Finding Undervalued Gems in the Indian Market</text>
  
  <!-- Graphics for value investing -->
  <!-- Stock price line -->
  <path d="M100,300 C150,280 200,270 250,260 C300,250 350,180 400,200 C450,220 500,230 550,160 C600,90 650,100 700,80" 
        fill="none" stroke="#3b82f6" stroke-width="3"/>
  
  <!-- Intrinsic value line -->
  <path d="M100,230 C150,225 200,222 250,220 C300,215 350,210 400,205 C450,200 500,195 550,190 C600,185 650,180 700,175" 
        fill="none" stroke="#22c55e" stroke-width="3" stroke-dasharray="5,5"/>
  
  <!-- Value zones -->
  <rect x="250" y="220" width="100" height="40" fill="#22c55e" fill-opacity="0.3" rx="5" />
  <text x="270" y="245" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Undervalued</text>
  
  <rect x="540" y="150" width="120" height="40" fill="#ef4444" fill-opacity="0.3" rx="5" />
  <text x="565" y="175" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Overvalued</text>
  
  <!-- Legend -->
  <circle cx="100" cy="350" r="6" fill="#3b82f6" />
  <text x="115" y="355" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Stock Price</text>
  
  <circle cx="250" cy="350" r="6" fill="#22c55e" />
  <text x="265" y="355" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Intrinsic Value</text>
  
  <!-- Annotations -->
  <circle cx="400" cy="200" r="8" fill="#ffffff" />
  <text x="390" y="180" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Buy</text>
  
  <circle cx="580" cy="135" r="8" fill="#ffffff" />
  <text x="570" y="115" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Sell</text>
</svg> 