<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#15803d" />
      <stop offset="100%" stop-color="#0f172a" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Sectoral Rotation</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#94a3b8">Capitalizing on Economic Cycles for Market Outperformance</text>
  
  <!-- Graphics for sectoral rotation -->
  <!-- Economic cycle circle -->
  <circle cx="400" cy="220" r="150" fill="none" stroke="#ffffff" stroke-width="2" />
  
  <!-- Phase markers on the circle -->
  <circle cx="400" cy="70" r="8" fill="#ffffff" />
  <circle cx="550" cy="220" r="8" fill="#ffffff" />
  <circle cx="400" cy="370" r="8" fill="#ffffff" />
  <circle cx="250" cy="220" r="8" fill="#ffffff" />
  
  <!-- Labels for economic phases -->
  <text x="400" y="50" font-family="Inter, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Early Recovery</text>
  <text x="580" y="220" font-family="Inter, sans-serif" font-size="16" fill="#ffffff" text-anchor="start">Expansion</text>
  <text x="400" y="390" font-family="Inter, sans-serif" font-size="16" fill="#ffffff" text-anchor="middle">Late Cycle</text>
  <text x="220" y="220" font-family="Inter, sans-serif" font-size="16" fill="#ffffff" text-anchor="end">Recession</text>
  
  <!-- Sector recommendations -->
  <rect x="330" y="100" width="140" height="50" rx="10" fill="#ffffff" fill-opacity="0.2" />
  <text x="345" y="120" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Financial Services</text>
  <text x="345" y="140" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Consumer Cyclicals</text>
  
  <rect x="480" y="190" width="140" height="50" rx="10" fill="#ffffff" fill-opacity="0.2" />
  <text x="495" y="210" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Industrial</text>
  <text x="495" y="230" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Technology</text>
  
  <rect x="330" y="290" width="140" height="50" rx="10" fill="#ffffff" fill-opacity="0.2" />
  <text x="345" y="310" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Energy</text>
  <text x="345" y="330" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Basic Materials</text>
  
  <rect x="180" y="190" width="140" height="50" rx="10" fill="#ffffff" fill-opacity="0.2" />
  <text x="195" y="210" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Utilities</text>
  <text x="195" y="230" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Consumer Staples</text>
  
  <!-- Rotation arrow along the circle -->
  <path d="M400,70 A150,150 0 0,1 550,220" fill="none" stroke="#38bdf8" stroke-width="4" />
  <path d="M550,220 A150,150 0 0,1 400,370" fill="none" stroke="#38bdf8" stroke-width="4" />
  <path d="M400,370 A150,150 0 0,1 250,220" fill="none" stroke="#38bdf8" stroke-width="4" />
  <path d="M250,220 A150,150 0 0,1 380,75" fill="none" stroke="#38bdf8" stroke-width="4" />
  
  <!-- Arrow head -->
  <path d="M385,80 L400,70 L380,55" fill="none" stroke="#38bdf8" stroke-width="4" />
  
  <!-- Legend -->
  <rect x="600" y="300" width="150" height="70" rx="10" fill="#ffffff" fill-opacity="0.1" />
  <text x="620" y="330" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Sector Rotation</text>
  <path d="M620,345 L680,345" stroke="#38bdf8" stroke-width="4" />
  <path d="M675,340 L685,345 L675,350" fill="none" stroke="#38bdf8" stroke-width="4" />
</svg> 