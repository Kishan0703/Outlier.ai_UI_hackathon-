<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#4338ca" />
      <stop offset="100%" stop-color="#1e1b4b" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Small-Cap Investing</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#a5b4fc">Finding Multi-Baggers in the Small-Cap Universe</text>
  
  <!-- Graphics for small-cap investing -->
  <!-- Large-cap symbol -->
  <circle cx="200" cy="220" r="80" fill="#8b5cf6" fill-opacity="0.3" stroke="#8b5cf6" stroke-width="2" />
  <text x="162" y="225" font-family="Inter, sans-serif" font-size="18" fill="#ffffff">Large Cap</text>
  
  <!-- Mid-cap symbol -->
  <circle cx="400" cy="220" r="60" fill="#f43f5e" fill-opacity="0.3" stroke="#f43f5e" stroke-width="2" />
  <text x="372" y="225" font-family="Inter, sans-serif" font-size="18" fill="#ffffff">Mid Cap</text>
  
  <!-- Small-cap symbol -->
  <circle cx="600" cy="220" r="40" fill="#10b981" fill-opacity="0.3" stroke="#10b981" stroke-width="2" />
  <text x="568" y="225" font-family="Inter, sans-serif" font-size="18" fill="#ffffff">Small Cap</text>
  
  <!-- Growth arrows -->
  <path d="M160,310 L160,280 L140,280 L180,250 L220,280 L200,280 L200,310 Z" fill="#8b5cf6" fill-opacity="0.7" />
  <text x="170" y="335" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">1x</text>
  
  <path d="M360,330 L360,260 L340,260 L380,230 L420,260 L400,260 L400,330 Z" fill="#f43f5e" fill-opacity="0.7" />
  <text x="370" y="355" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">3x</text>
  
  <path d="M560,350 L560,240 L540,240 L580,210 L620,240 L600,240 L600,350 Z" fill="#10b981" fill-opacity="0.7" />
  <text x="570" y="375" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">5-10x</text>
  
  <!-- Annotation -->
  <path d="M650,220 L730,170" stroke="#ffffff" stroke-width="2" stroke-dasharray="3,3" />
  <rect x="620" y="120" width="200" height="50" rx="10" fill="#ffffff" fill-opacity="0.1" />
  <text x="640" y="150" font-family="Inter, sans-serif" font-size="14" fill="#ffffff">Higher Risk, Higher Return</text>
</svg> 