<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#7e22ce" />
      <stop offset="100%" stop-color="#0f172a" />
    </linearGradient>
  </defs>
  
  <!-- Background rectangle -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="50" y="80" font-family="Inter, sans-serif" font-size="36" font-weight="bold" fill="#ffffff">Technical Analysis</text>
  
  <!-- Subtitle -->
  <text x="50" y="120" font-family="Inter, sans-serif" font-size="18" fill="#a5b4fc">Chart Patterns Every Indian Investor Should Know</text>
  
  <!-- Graphics for technical analysis -->
  <!-- Chart background -->
  <rect x="100" y="150" width="600" height="180" fill="#ffffff" fill-opacity="0.05" />
  
  <!-- Grid lines -->
  <line x1="100" y1="190" x2="700" y2="190" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="100" y1="230" x2="700" y2="230" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="100" y1="270" x2="700" y2="270" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="100" y1="310" x2="700" y2="310" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  
  <line x1="150" y1="150" x2="150" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="250" y1="150" x2="250" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="350" y1="150" x2="350" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="450" y1="150" x2="450" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="550" y1="150" x2="550" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  <line x1="650" y1="150" x2="650" y2="330" stroke="#64748b" stroke-width="1" stroke-dasharray="3,3" />
  
  <!-- Stock price line chart -->
  <path d="M100,260 L150,240 L200,250 L250,230 L300,190 L350,210 L400,170 L450,190 L500,240 L550,220 L600,260 L650,240 L700,210" 
        fill="none" stroke="#38bdf8" stroke-width="3" />
  
  <!-- Candlestick patterns -->
  <!-- Head and shoulders pattern highlight -->
  <path d="M250,175 C270,140 290,150 310,175 C330,200 350,210 370,175 C390,140 410,150 430,175 C450,200 470,210 490,175" 
        fill="none" stroke="#f43f5e" stroke-width="2" stroke-dasharray="5,3" />
  <text x="360" y="130" font-family="Inter, sans-serif" font-size="12" fill="#f43f5e" text-anchor="middle">Head and Shoulders</text>
  
  <!-- Support line -->
  <line x1="100" y1="270" x2="700" y2="270" stroke="#22c55e" stroke-width="2" />
  <text x="710" y="275" font-family="Inter, sans-serif" font-size="12" fill="#22c55e">Support</text>
  
  <!-- Resistance line -->
  <line x1="250" y1="170" x2="550" y2="170" stroke="#ef4444" stroke-width="2" />
  <text x="560" y="175" font-family="Inter, sans-serif" font-size="12" fill="#ef4444">Resistance</text>
  
  <!-- Volume bars -->
  <rect x="100" y="350" width="20" height="10" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="150" y="350" width="20" height="15" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="200" y="350" width="20" height="8" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="250" y="350" width="20" height="20" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="300" y="350" width="20" height="30" fill="#38bdf8" fill-opacity="0.7" />
  <rect x="350" y="350" width="20" height="15" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="400" y="350" width="20" height="25" fill="#38bdf8" fill-opacity="0.7" />
  <rect x="450" y="350" width="20" height="12" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="500" y="350" width="20" height="18" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="550" y="350" width="20" height="22" fill="#38bdf8" fill-opacity="0.7" />
  <rect x="600" y="350" width="20" height="15" fill="#38bdf8" fill-opacity="0.5" />
  <rect x="650" y="350" width="20" height="25" fill="#38bdf8" fill-opacity="0.7" />
  
  <!-- Label for volume -->
  <text x="90" y="365" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Vol</text>
  
  <!-- Technical indicators -->
  <!-- Moving averages -->
  <path d="M100,280 L150,270 L200,260 L250,240 L300,225 L350,210 L400,200 L450,205 L500,215 L550,225 L600,240 L650,245 L700,250" 
        fill="none" stroke="#f59e0b" stroke-width="2" stroke-dasharray="3,0" />
  
  <path d="M100,290 L150,285 L200,275 L250,260 L300,250 L350,240 L400,230 L450,225 L500,230 L550,235 L600,240 L650,250 L700,260" 
        fill="none" stroke="#a855f7" stroke-width="2" stroke-dasharray="3,0" />
  
  <!-- Legend -->
  <rect x="600" y="150" width="90" height="60" rx="5" fill="#ffffff" fill-opacity="0.1" />
  <line x1="610" y1="165" x2="630" y2="165" stroke="#38bdf8" stroke-width="2" />
  <text x="640" y="170" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">Price</text>
  
  <line x1="610" y1="185" x2="630" y2="185" stroke="#f59e0b" stroke-width="2" />
  <text x="640" y="190" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">20 MA</text>
  
  <line x1="610" y1="205" x2="630" y2="205" stroke="#a855f7" stroke-width="2" />
  <text x="640" y="210" font-family="Inter, sans-serif" font-size="12" fill="#ffffff">50 MA</text>
</svg> 